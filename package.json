    "lint": "eslint --ignore-path .gitignore . --fix",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@aws-cdk/assert": "^1.130.0",
    "@aws-cdk/assertions": "^1.131.0",
    "@types/jest": "^26.0.10",
