    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "aws-cdk": "1.130.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.2.0",
    "jest": "^27.3.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^9.0.0",
    "typescript": "~3.9.7",
    "husky": "^7.0.0"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "^1.131.0",
