    "@aws-cdk/aws-kms": "^1.131.0",
    "@aws-cdk/aws-lambda": "^1.131.0",
    "@aws-cdk/aws-s3": "^1.131.0",
    "@aws-cdk/aws-secretsmanager": "^1.131.0",
    "@aws-cdk/aws-sqs": "^1.131.0",
    "@aws-cdk/core": "^1.89.0",
    "source-map-support": "^0.5.16"
